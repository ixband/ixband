---
layout: null
---

{
  "docs": [
    {% assign docLength = site.docs.length %}
    {% for doc in site.docs %}
      {
        "title": {{ doc.title }},
        "category": {{ doc.category }},
        "tags": {{ doc.tags }},
        "path": {{ site.baseurl }}{{ doc.id }},
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}