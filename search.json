---
layout: null
---

{
  "docs": [
    {% for doc in site.docs %}
      {% assign data=site.data[doc.category][doc.title] %}
      {
        "title": "{{ data.title }}",
        "description": [
          {% for desc in data.description %}
            "{{ desc | smartify }}"{% unless forloop.last %},{% endunless %}
          {% endfor %}
        ],
        "category": "{{ doc.category }}",
        "tags": [
          {% for tag in doc.tags %}
            "{{ tag | smartify }}"{% unless forloop.last %},{% endunless %}
          {% endfor %}
        ],
        "path": "{{ site.baseurl }}{{ doc.id }}"
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}