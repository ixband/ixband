---
layout: null
sitemap: false
---

<?xml version="1.0" encoding="utf-8"?>
<search>
    {% for page in site.docs %}
	{% assign data=site.data[page.category][page.title] %}
    <entry>
        <title><![CDATA[{{ data.title }}]]></title>
        <url>{{ site.url }}{{ page.url | relative_url }}</url>
        <content type="html"><![CDATA[{{ data.description }}]]></content>
        <categories>
		{% for category in page.categories %}
			{% if category == 'B' %}
				<category>$B</category>
			{% else %}
				<category>{{ category }}</category>
			{% endif %}
		{% endfor %}
        </categories>
        <tags>
            {% for tag in page.tags %}
            <tag> {{ tag }} </tag>
            {% endfor %}
        </tags>
    </entry>
    {% endfor %}
</search>